extends layout

block content
  .container
    .row
    .col-sm-6
      h1 Pokemon is life
      p.lead A site for everything pokemon
    .col-sm-6
      img(src="data:image/jpeg;base64,", alt="pokemonLogo")  
      hr
    .row
      .col-sm-12
        h2 Find pokemon using the game they are in
        p
        .form-group
          form.form-horizontal(method='POST')
            input(type='hidden', name='_csrf', value=_csrf)
            .form-group
              label.col-sm-3.control-label(for='game') Game
              .col-sm-7
                input.form-control(type='game', name='game', id='game', placeholder='red', autofocus, required)
            .form-group
              .col-sm-offset-3.col-sm-7
                button.col-sm-3.btn.btn-primary(type='submit')
                  i.fa.fa-play
                  | Search
      .col-sm-12

    h3 Resutls
    table.table
      thead
        tr
          th №
          th Name
          th Height
          th Weight
          th Image 
      tbody
      for each in pokies
        tr
         a(href=each.location_area_encounters)
          td= each.id
          td!= each.name
          td!= each.height
          td!= each.weight
          <td> 
            <img src="(each.sprite)", alt="image" />
          </td>

